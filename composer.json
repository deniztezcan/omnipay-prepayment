{
    "name": "deniztezcan/omnipay-prepayment",
    "description": "Prepayment driver for the Omnipay PHP payment processing library",
    "type": "library",
    "keywords": [
        "gateway",
        "merchant",
        "omnipay",
        "pay",
        "payment",
        "prepayment"
    ],
    "version": "1.0.0",
    "homepage": "https://github.com/deniztezcan/omnipay-prepayment",
    "license": "MIT",
    "authors": [
        {
            "name": "Deniz Tezcan",
            "email": "howdy@deniztezcan.me"
        }
    ],
    "autoload": {
        "psr-4": { "Omnipay\\PrePayment\\" : "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Omnipay\\PrePayment\\Tests\\": "tests/" }
    },
    "require": {
        "league/omnipay": "^3",
        "spatie/array-to-xml": "2.8.*",
        "nesbot/carbon": "2.*"
    },
    "require-dev": {
        "omnipay/tests": "^3"
    },
    "prefer-stable": true
}
